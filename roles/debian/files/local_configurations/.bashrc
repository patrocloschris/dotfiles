
# History file format
export HISTTIMEFORMAT="%d/%m/%y %T "
export HISTSIZE=-1
export HISTFILESIZE=-1

# File manipulation
alias rm='rm -i'

# Grep
alias grep='grep --color=auto'

# List
alias ll='ls -la'
alias ls='ls --color=auto'

# Dir
alias mkdir='mkdir -pv'

# Json
alias json="python -m json.tool"

# Open Ports
alias ports='netstat -tulanp'

# Images
alias gallery='sxiv'
